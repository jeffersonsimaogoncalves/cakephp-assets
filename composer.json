{
  "name": "jeffersonsimaogoncalves/cakephp-assets",
  "description": "Assets plugin for CakePHP",
  "homepage": "https://github.com/jeffersonsimagoncalves/cakephp-assets",
  "type": "cakephp-plugin",
  "license": "MIT",
  "authors": [
    {
      "name": "Mirko Pagliai",
      "email": "mirko.pagliai@gmail.com"
    },
    {
      "name": "Jefferson Simão Gonçalves",
      "homepage": "https://github.com/jeffersonsimaogoncalves"
    }
  ],
  "require": {
    "php": ">=5.5.9",
    "cakephp/cakephp": "^3.7",
    "jeffersonsimaogoncalves/php-tools": "^1.3",
    "matthiasmullie/minify": "^1.3"
  },
  "require-dev": {
    "cakephp/cakephp-codesniffer": "^3.0",
    "phpunit/phpunit": "^5.7.14|^6.0"
  },
  "autoload": {
    "psr-4": {
      "JeffersonSimaoGoncalves\\Assets\\": "src"
    }
  },
  "prefer-stable": true,
  "config": {
    "sort-packages": true,
    "htaccess-protect": true,
    "optimize-autoloader": true
  }
}
